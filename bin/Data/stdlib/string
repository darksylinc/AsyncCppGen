
#ifndef stdlib_string
#define stdlib_string

namespace std
{
	template <typename _CharT>
	class basic_string
	{
	};

	class string
	{
	public:
		string();
		string( const char * );

		bool operator<( const std::string & ) const;
		bool operator<( const char * ) const;

		friend bool operator<( const char *lhs, const string &rhs );
	};
}  // namespace std

#endif
